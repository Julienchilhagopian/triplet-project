{{#if category}}
<section class="header-category-org-list">
    <div>
        <img src="/images/{{category}}.svg" alt="">
    </div>
    <h1>{{category}}</h1>
</section>
{{/if}} {{#unless category}}
<h1>All categories</h1>
{{/unless}}
<div class="all-org-list">
    {{#each users}} {{#if this.isActive}}

    <div class="detail-card">
        <img src="http://imgs.globovision.com/joHKNsQpCPpOmSbZX0RLmwUPaHU=/847x0/smart/9e28a2ebfcb34ed5bf71e5c093532f33" alt="">
        <div class="org-description">
            <h2>{{this.username}}</h2>
            <p>{{this.description}}</p>
            <a href="org/{{this._id}}">
                <button>
                    See more
                </button>
            </a>
        </div>
    </div>
    {{/if}} {{/each}}
</div>