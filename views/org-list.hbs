{{#each category}} {{this.category}} {{/each}}

<div class="all-org-list">
    {{#each users}}
    <div class="detail-card">
        <div class="detail-card-img">
            <img src="{{this.imgUrl}}" alt="">
        </div>
        <div class="org-description">
            <h2>{{this.username}}</h2>
            <p>{{this.description}}</p>
            <a href="org/{{this._id}}">
                <button>
                    See more
                </button>
            </a>
        </div>
    </div>
    {{/each}}
</div>